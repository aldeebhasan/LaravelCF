{
  "name": "aldeebhasan/laravelcf",
  "description": "This package will allow you to make fast recommendation based on custom inputs",
  "type": "library",
  "version": "1.0.3",
  "license": "MIT",
  "keywords": [
    "recommendation",
    "fast recommendation",
    "collaborative filtering",
    "product recommendations",
    "statistics",
    "laravel"
  ],
  "authors": [
    {
      "name": "Hasan Deeb",
      "email": "aldeeb.91@gmail.com",
      "homepage": "https://hasan-deeb.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Aldeebhasan\\LaravelCF\\": "src/"
    },
    "files": [
      "src/Utilities/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Aldeebhasan\\LaravelCF\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "orchestra/testbench": "8.x-dev"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Aldeebhasan\\LaravelCF\\LaravelCFProvider"
      ],
      "aliases": {
      }
    }
  },
  "scripts": {
    "fix": "php-cs-fixer fix src"
  }
}
